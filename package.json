{
  "name": "tukod",
  "version": "0.0.0-alpha.2",
  "description": "CLI manager for vex-turbo-boilerplate",
  "type": "module",
  "private": true,
  "scripts": {
    "fi": "npx rimraf **/node_modules/** && npx rimraf yarn.lock && yarn install",
    "build": "turbo run build --color --filter=cli",
    "lint": "turbo run lint --color --filter=cli",
    "pub": "turbo run build pub --color --filter=cli --filter=scripts",
    "pub:dev": "turbo run build pub:dev --color --filter=cli --filter=scripts"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/vexCoder/tukod/issues"
  },
  "homepage": "https://github.com/vexCoder/tukod#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vexCoder/tukod.git"
  },
  "workspaces": {
    "packages": [
      "cli",
      "scripts"
    ]
  },
  "keywords": [],
  "author": "vexCoder <freelance.starterpack08@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "turbo": "1.6.3",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "typescript": "4.6.2"
  },
  "main": "index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "type-fest": "^2.12.0"
  }
}
